def application(environ, start_response):
	start_response('200 Ok', [('Content-Type', 'text/plan')])
	message = 'It works! \n'
	version = 'Python %s \n' % sys.version.split()[0]
	response = '\n'.join([message, version])
	return [response.encode()]

